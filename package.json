{
  "name": "tales",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src test",
    "test": "jest",
    "format": "prettier --write 'src/**/*.js' 'src/**/*.jsx' 'test/**/*.js'",
    "dev": "NODE_ENV=development webpack-dev-server --mode=development",
    "build": "NODE_ENV=production webpack-cli -p",
    "precommit": "lint-staged",
    "prepush": "npm test"
  },
  "keywords": [],
  "dependencies": {
    "prop-types": "15.6.2",
    "react": "16.4.1",
    "react-dom": "16.4.1"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.6",
    "babel-loader": "7.1.5",
    "babel-preset-env": "1.7.0",
    "babel-preset-react-app": "3.1.2",
    "eslint": "5.0.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-react-app": "2.1.0",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-react": "7.10.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "0.14.3",
    "jest": "23.3.0",
    "lint-staged": "7.2.0",
    "prettier": "1.13.7",
    "react-test-renderer": "16.4.1",
    "webpack": "4.15.1",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.4"
  },
  "lint-staged": {
    "*.{js,jsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "src/**/*.jsx",
      "!src/**/__tests__/**/*"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "roots": [
      "<rootDir>/src"
    ]
  }
}
